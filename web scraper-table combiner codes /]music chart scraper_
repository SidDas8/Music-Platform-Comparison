\n","songs = []\n","artists = []\n","week = []\n","rank = []\n","\n","##for y in years:\n","  year_url = \"/singles-charts/\"+y\n","  html_year = html_getter (year_url)\n","  tree_2 = html.fromstring(html_year.text)\n","  aa = tree_2.xpath('/html/body/div[2]/div/div[3]/main/div/section/div/div/article/div/div[1]/div/div')\n","  urls_chart_week = [url.attrib['href'] for url in aa[0].xpath('.//div[@class=\"chart-month-list\"]/a')]\n","\n","  ##for url  in urls_chart_week:\n","      table_xpath = '//*[@id=\"block-system-main\"]/div/div/table//tr'\n","      song_xpath = './/td[2]//text()'\n","      artist_xpath = './/td[3]//text()'\n","      date_part = url.split('/')[-1]\n","      html_web = html_getter(url)\n","\n","      tree = html.fromstring(html_web.text)\n","      table = tree.xpath(table_xpath)\n","\n","      for i in range(50):\n","          song = table[i].xpath(song_xpath)\n","          artist = table[i].xpath(artist_xpath)\n","          songs.append(','.join(song))\n","          artists.append(','.join(artist))\n","          rank.append(i+1)\n","          week.append(date_part)"],"metadata":{"id":"YxYsX2PDMcrJ"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["df = pd.DataFrame({\n","    'Song': songs,\n","    'Artist': artists,\n","    'Week':week,\n","    'Rank':rank\n","})\n","\n","df\n","#df.to_csv('music_chart_data.csv',index = False)"],"metadata":{"id":"eSOEVhwJMhm4"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["Here now we are getting the data stragit from the CSV file\n"],"metadata":{"id":"89rG0MDG37cT"}},{"cell_type":"code","source":["df =pd.read_csv('/content/drive/MyDrive/sta 142b codes file /music_chart_data.csv')\n","df"],"metadata":{"id":"RdJnT1ynyXst","colab":{"base_uri":"https://localhost:8080/","height":424},"executionInfo":{"status":"ok","timestamp":1702254475068,"user_tz":480,"elapsed":342,"user":{"displayName":"Kevin Torres","userId":"02770742881997735716"}},"outputId":"47920788-2231-422f-e95b