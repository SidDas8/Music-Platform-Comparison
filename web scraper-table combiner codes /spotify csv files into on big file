 of the data in here\n"],"metadata":{"id":"LFd-ZjpvPEh_"}},{"cell_type":"code","source":["\n","\n","# Assuming your DataFrame is named 'df'\n","# If your 'WeekDate' column is not already in datetime format, convert it\n","df['WeekDate'] = pd.to_datetime(df['WeekDate'])\n","\n","# Split songs with multiple artists into separate rows\n","df['artist_names'] = df['artist_names'].str.split(', ')\n","df = df.explode('artist_names')\n","\n","# Group by 'artist_names' and 'WeekDate' and count the occurrences\n","artist_counts = df.groupby(['artist_names', 'WeekDate']).size().reset_index(name='count')\n","\n","# Filter artists with more than one appearance\n","\n","song_ons_chart = 10\n","selected_artists = artist_counts[artist_counts['count'] > song_ons_chart]\n","\n","# Plotting the histogram\n","plt.figure(figsize=(10, 6))\n","plt.hist(selected_artists['count'], bins=20, color='skyblue', edgecolor='black')\n","plt.title(f' Spotify Number of songs an artist had on a Weekly Chart (Appearing More Than {song_ons_chart} times)')\n","plt.xlabel('Number of Appearances')\n","plt.ylabel('Number of Artists')\n","plt.grid(axis='y', linestyle='--', alpha=0.7)\n","\n","#\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":564},"id":"J8cI1QpOQ6n9","executionInfo":{"status":"ok","timestamp":1702599769281,"user_tz":480,"elapsed":734,"user":{"displayName":"Kevin Torres","userId":"02770742881997735716"}},"outputId":"6698fd97-ce85-40ba-df43-d9c60e7d04c9"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["import pandas as pd\n","import plotly.express as px\n","\n","# Assuming df is your DataFrame and you have it loaded with the necessary data\n","artist_name = \"Doja Cat\"\n","filter_song_rank = df[df['artist_names'].str.contains(artist_name, case=False)]\n","\n","# Convert 'WeekDate' to datetime for proper plotting\n","